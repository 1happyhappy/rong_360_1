    "import joblib\n",
    "import pandas as pd\n",
    "\n",
    "xgb_model = joblib.load('xgb_model')\n",
    "feature_names = joblib.load('feature_names')\n",
    "\n",
    "f_score = pd.Series(index = feature_names,data=xgb_model.feature_importances_).sort_values(ascending=False)\n",
    "\n",
    "joblib.dump(f_score,'f_score')"
   ]
